                                Проверка полиса ОСАГО
                            .list-block
                                ul 
                                    li
                                        a(href="#").item-link.smart-select
                                            select(name="serial")
                                                option(value="XXX", selected=true) XXX
                                                option(value="CCC") CCC
                                                option(value="EEE") EEE
                                                option(value="BBB") BBB
                                            .item-content
                                                .item-media
                                                    i.icon.icon-form-name
                                                .item-inner
                                                    .item-title Серия
                                    li
                                        .item-content
                                            .item-media
                                                i.icon.icon-form-name
